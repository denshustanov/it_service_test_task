#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/deniel/IdeaProjects/it-service_test-task/node_modules/.pnpm/registry.npmjs.org/mime/2.4.6/node_modules/mime/cli.js/node_modules:/home/deniel/IdeaProjects/it-service_test-task/node_modules/.pnpm/registry.npmjs.org/mime/2.4.6/node_modules/mime/node_modules:/home/deniel/IdeaProjects/it-service_test-task/node_modules/.pnpm/registry.npmjs.org/mime/2.4.6/node_modules:/home/deniel/IdeaProjects/it-service_test-task/node_modules/.pnpm/registry.npmjs.org/mime/node_modules:/home/deniel/IdeaProjects/it-service_test-task/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/deniel/IdeaProjects/it-service_test-task/node_modules/.pnpm/node_modules:/home/deniel/IdeaProjects/it-service_test-task/node_modules:/home/deniel/IdeaProjects/node_modules:/home/deniel/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../mime/2.4.6/node_modules/mime/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/deniel/IdeaProjects/it-service_test-task/node_modules/.pnpm/registry.npmjs.org/mime/2.4.6/node_modules/mime/cli.js/node_modules:/home/deniel/IdeaProjects/it-service_test-task/node_modules/.pnpm/registry.npmjs.org/mime/2.4.6/node_modules/mime/node_modules:/home/deniel/IdeaProjects/it-service_test-task/node_modules/.pnpm/registry.npmjs.org/mime/2.4.6/node_modules:/home/deniel/IdeaProjects/it-service_test-task/node_modules/.pnpm/registry.npmjs.org/mime/node_modules:/home/deniel/IdeaProjects/it-service_test-task/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/deniel/IdeaProjects/it-service_test-task/node_modules/.pnpm/node_modules:/home/deniel/IdeaProjects/it-service_test-task/node_modules:/home/deniel/IdeaProjects/node_modules:/home/deniel/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../mime/2.4.6/node_modules/mime/cli.js" "$@"
  ret=$?
fi
exit $ret
